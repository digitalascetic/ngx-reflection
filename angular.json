{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@digitalascetic/ngx-reflection": {
      "root": "projects/digitalascetic/ngx-reflection",
      "sourceRoot": "projects/digitalascetic/ngx-reflection/src",
      "projectType": "library",
      "prefix": "ascre",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/digitalascetic/ngx-reflection/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/digitalascetic/ngx-reflection/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/digitalascetic/ngx-reflection/src/test.ts",
            "tsConfig": "projects/digitalascetic/ngx-reflection/tsconfig.spec.json",
            "karmaConfig": "projects/digitalascetic/ngx-reflection/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/digitalascetic/ngx-reflection/tsconfig.lib.json",
              "projects/digitalascetic/ngx-reflection/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "@digitalascetic/ngx-reflection"
}
